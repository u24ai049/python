def ispangram(string2):
    se=len(set(string2))
    if 26==se:
        print("Is Pangram")
    else:
        print("Is Not Pangram")
            
T=int(input("Enter the number of test cases : "))
for i in range(T):
    string=input("Enter the string : ")
    string.strip()
    string2=string.lower()
    ispangram(string2)
