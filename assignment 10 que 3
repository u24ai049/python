import numpy as np

def magic_square(N):
    if N % 2 != 0:  # Odd order magic square
        square = np.zeros((N, N), dtype=int)
        i, j = 0, N // 2
        for num in range(1, N*N + 1):
            square[i, j] = num
            new_i, new_j = (i - 1) % N, (j + 1) % N
            if square[new_i, new_j]:
                i += 1
            else:
                i, j = new_i, new_j
        return square
    else:  # For simplicity, we handle only odd N for now
        return None

def print_square(square):
    for row in square:
        print(' '.join(str(x) for x in row))
    print()

# Taking user input for N (order of the magic square)
N = int(input("Enter the order (N) for the magic square (odd number): "))
square = magic_square(N)
if square is not None:
    print(f"Magic Square for N={N}:")
    print_square(square)
else:
    print(f"Magic Square for {N} is not handled.")
